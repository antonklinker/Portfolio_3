<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="350.0" prefWidth="350.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="loginapp.LoginController">
   <children>
      <TextField fx:id="usernamefield" layoutX="112.0" layoutY="68.0" />
      <PasswordField fx:id="passwordfield" layoutX="112.0" layoutY="113.0" />
      <Button fx:id="loginbutton" layoutX="112.0" layoutY="213.0" mnemonicParsing="false" onAction="#loginMethod" prefHeight="25.0" prefWidth="149.0" text="Login" />
      <Label layoutX="48.0" layoutY="14.0" text="DB Status" />
      <Label fx:id="dbstatus" layoutX="112.0" layoutY="14.0" />
      <Label layoutX="40.0" layoutY="72.0" text="Username" />
      <Label layoutX="40.0" layoutY="117.0" text="Password" />
      <ComboBox fx:id="divisioncombobox" layoutX="112.0" layoutY="157.0" prefWidth="150.0" promptText="Select division" />
      <Label layoutX="39.0" layoutY="161.0" text="Division" />
      <Label fx:id="loginStatus" layoutX="14.0" layoutY="260.0" />
      <Button layoutX="12.0" layoutY="311.0" mnemonicParsing="false" onAction="#bypassLogin" text="BYPASS LOGIN" />
   </children>
</AnchorPane>

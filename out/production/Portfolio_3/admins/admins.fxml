<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="720.0" prefWidth="1280.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="admins.AdminsController">
   <children>
      <TabPane prefHeight="720.0" prefWidth="1280.0" tabClosingPolicy="UNAVAILABLE">
        <tabs>
            <Tab text="Students">
              <content>
                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <HBox layoutX="287.0" prefHeight="692.0" prefWidth="995.0">
                           <children>
                              <TableView fx:id="_studenttable" prefHeight="692.0" prefWidth="995.0">
                                <columns>
                                  <TableColumn fx:id="_idcolumn" prefWidth="75.0" text="ID" />
                                  <TableColumn fx:id="_namecolumn" prefWidth="397.0" text="Name" />
                                </columns>
                              </TableView>
                           </children>
                        </HBox>
                        <VBox layoutX="6.0" prefHeight="691.0" prefWidth="276.0">
                           <children>
                              <Button mnemonicParsing="false" onAction="#moreInformation" text="Additional information">
                                 <VBox.margin>
                                    <Insets left="50.0" top="40.0" />
                                 </VBox.margin>
                              </Button>
                           </children>
                        </VBox>
                     </children>
                  </AnchorPane>
              </content>
            </Tab>
          <Tab text="Add new student">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <HBox layoutX="339.0" prefHeight="692.0" prefWidth="943.0">
                           <children>
                              <TableView fx:id="studenttable" prefHeight="692.0" prefWidth="947.0">
                                <columns>
                                  <TableColumn fx:id="idcolumn" prefWidth="75.0" text="ID" />
                                  <TableColumn fx:id="firstnamecolumn" prefWidth="159.0" text="First name" />
                                    <TableColumn fx:id="lastnamecolumn" minWidth="0.0" prefWidth="241.0" text="Last name" />
                                    <TableColumn fx:id="emailcolumn" minWidth="0.0" prefWidth="352.0" text="Email" />
                                    <TableColumn fx:id="dobcolumn" prefWidth="114.0" text="Date of Birth" />
                                </columns>
                              </TableView>
                           </children>
                        </HBox>
                        <VBox layoutY="12.0" prefHeight="683.0" prefWidth="340.0">
                           <children>
                              <Label prefHeight="30.0" prefWidth="310.0" text="Add Students">
                                 <padding>
                                    <Insets left="50.0" />
                                 </padding>
                              </Label>
                              <TextField fx:id="id" promptText="ID">
                                 <VBox.margin>
                                    <Insets left="15.0" right="15.0" />
                                 </VBox.margin>
                              </TextField>
                              <TextField fx:id="firstname" layoutX="10.0" layoutY="27.0" promptText="First name">
                                 <VBox.margin>
                                    <Insets left="15.0" right="15.0" />
                                 </VBox.margin>
                              </TextField>
                              <TextField fx:id="lastname" layoutX="10.0" layoutY="52.0" promptText="Last name">
                                 <VBox.margin>
                                    <Insets left="15.0" right="15.0" />
                                 </VBox.margin>
                              </TextField>
                              <TextField fx:id="email" layoutX="10.0" layoutY="77.0" promptText="Email">
                                 <VBox.margin>
                                    <Insets left="15.0" right="15.0" />
                                 </VBox.margin>
                              </TextField>
                              <DatePicker fx:id="dob" prefHeight="25.0" prefWidth="326.0">
                                 <VBox.margin>
                                    <Insets left="15.0" right="15.0" />
                                 </VBox.margin>
                              </DatePicker>
                              <Button mnemonicParsing="false" onAction="#addStudent" prefHeight="25.0" prefWidth="280.0" text="Add Entry">
                                 <VBox.margin>
                                    <Insets left="15.0" />
                                 </VBox.margin>
                              </Button>
                              <Button layoutX="40.0" layoutY="152.0" mnemonicParsing="false" onAction="#clearForm" prefHeight="25.0" prefWidth="280.0" text="Clear Form">
                                 <VBox.margin>
                                    <Insets left="15.0" />
                                 </VBox.margin>
                              </Button>
                              <Button layoutX="25.0" layoutY="177.0" mnemonicParsing="false" onAction="#loadStudentData" prefHeight="25.0" prefWidth="280.0" text="Load Data">
                                 <VBox.margin>
                                    <Insets left="15.0" />
                                 </VBox.margin>
                              </Button>
                           </children>
                           <padding>
                              <Insets left="15.0" right="15.0" />
                           </padding>
                        </VBox>
                     </children></AnchorPane>
            </content>
          </Tab>
        </tabs>
      </TabPane>
   </children>
</AnchorPane>
